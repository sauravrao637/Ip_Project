<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_analysis">

    <fragment
        android:id="@+id/navigation_analysis"
        android:name="com.camo.ip_project.ui.analysis.AnalysisFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_analysis" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.camo.ip_project.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_about_info"
        android:name="com.camo.ip_project.ui.info.AboutInfoFragment"
        android:label="@string/title_about"
        tools:layout="@layout/fragment_about_info" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.camo.ip_project.ui.settings.SettingsFragment"
        android:label="SettingsFragment" />
</navigation>